; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Flick"
run/main_scene="res://scenes/main_menu/_Main.tscn"
config/features=PackedStringArray("4.1")
config/icon="res://icon.png"

[autoload]

Level="*res://scripts/Level.gd"
Constants="*res://scripts/Constants.gd"
AnimaUI="*res://addons/anima/ui/AnimaUI.gd"
ANIMA="*res://addons/anima/core/constants.gd"
Anima="*res://addons/anima/core/anima.gd"
SavedData="*res://scripts/SavedData.gd"
Functions="*res://scripts/Functions.gd"

[debug]

settings/stdout/print_fps=true
gdscript/completion/autocomplete_setters_and_getters=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"

[editor]

movie_writer/movie_file="C:/Git/flick/exports/movies/flick_screencast.avi"
convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PackedStringArray("res://addons/anima/plugin.cfg")

[gui]

theme/custom_font="res://fonts/Chewy.ttf"
theme/use_hidpi=true

